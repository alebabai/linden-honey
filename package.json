{
  "name": "linden-honey",
  "version": "1.0.0",
  "description": "REST API for the lyrics of Yegor Letov and his post-punk/psychedelic rock-band Grazhdanskaya Oborona (Civil Defense)",
  "main": "src/index.js",
  "dependencies": {
    "koa": "^2.2.0",
    "koa-logger": "^2.0.1",
    "koa-router": "^7.2.0",
    "migrate-mongoose": "^3.2.2",
    "mongoose": "^4.9.3",
    "nconf": "^0.8.4",
    "node-fetch": "^1.6.3",
    "random-js": "^1.0.8"
  },
  "devDependencies": {
    "eslint": "^3.13.1"
  },
  "scripts": {
    "migrate": "node_modules/.bin/migrate up -d mongodb://$db_config_user:$db_config_password@$db_config_host:$db_config_port/$db_config_dbName --md src/migrations --es6 --autosync true",
    "start": "node src",
    "debug": "node --inspect src"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/alebabai/linden-honey.git"
  },
  "keywords": [
    "linden honey",
    "letov",
    "grob",
    "koajs",
    "mongo",
    "mongoose"
  ],
  "author": "Alexander Babai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alebabai/linden-honey/issues"
  },
  "homepage": "https://github.com/alebabai/linden-honey",
  "engines": {
    "node": "8.0.0"
  }
}
